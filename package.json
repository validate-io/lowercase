{
  "name": "validate.io-lowercase",
  "version": "1.0.0",
  "description": "Validates if a value is a lowercase string.",
  "author": {
    "name": "Athan Reines",
    "email": "kgryte@gmail.com"
  },
  "contributors": [
    {
      "name": "Athan Reines",
      "email": "kgryte@gmail.com"
    }
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/validate-io/lowercase.git"
  },
  "keywords": [
    "validate.io",
    "validate",
    "validation",
    "validator",
    "valid",
    "string",
    "lowercase"
  ],
  "bugs": {
    "url": "https://github.com/validate-io/lowercase/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "1.x.x",
    "mocha": "1.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ]
